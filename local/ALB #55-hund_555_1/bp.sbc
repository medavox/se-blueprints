 */

public Program()
{
    Runtime.UpdateFrequency = UpdateFrequency.Update1;
}
public void Main(string argument, UpdateType updateSource)
{
    // ------------------ INSTRUCTIONS ------------------

    // This script will lower your steering angle on the front wheels depending on the speed parameters you set.
    // This can help prevent the understeer issue where you just keep going in a straight line even if you try to slow down mid turn.

    // ------------------ CHANGE SETTINGS BELOW HERE ------------------


    // Names of wheel suspension

    string FrontLeftWheelName = "FL Front Left";          string FrontRightWheelName = "FR Front Right";


    string ControlSeatName = "Control Seat"; // Name of Control Seat/Remote Control.


    // ----------------- Steer Parameters. -----------------

    float MinimumSpeedThreshold = 65;  // Speed in m/s where the steering angle starts to decrease.
    float MaximumSpeedThreshold = 90; // Speed in m/s where the steering angle stops decreasing.

    float MaxSteerAngle = 44f;         // Your usual steering angle for the front wheels
    float MinSteerAngle = 32f;         // Minimum steering angle



    //--------------------------------------------------------------------------------------------

    // Dont touch below here or you will most likely break things.

    //--------------------------------------------------------------------------------------------

    float SpeedRange = (MaximumSpeedThreshold - MinimumSpeedThreshold);

    float SteerRange = MaxSteerAngle - MinSteerAngle;
    float SteerMultiplier = (SpeedRange / SteerRange);
    float DesiredAngle;

    IMyMotorSuspension FL;
    FL = GridTerminalSystem.GetBlockWithName(FrontLeftWheelName) as IMyMotorSuspension;
    if (FL == null)
    {
        Echo("Front Left Suspension Missing");
    }
    IMyMotorSuspension FR;
    FR = GridTerminalSystem.GetBlockWithName(FrontRightWheelName) as IMyMotorSuspension;
    if (FR == null)
    {
        Echo("Front Right Suspension Missing");
    }

    IMyShipController Cockpit;
    Cockpit = GridTerminalSystem.GetBlockWithName(ControlSeatName) as IMyShipController;
    if (Cockpit == null)
    {
        Echo("Control Seat/Remote Control Missing");
    }

    double CurrentSpeedDouble;
    float CurrentSpeed;
    // Steer Change
    //-------------------------------------------------
    if (Cockpit != null)
    {
        CurrentSpeedDouble = Cockpit.GetShipSpeed();
        CurrentSpeed = Convert.ToSingle(CurrentSpeedDouble);

        if (CurrentSpeed &gt; MinimumSpeedThreshold &amp;&amp; CurrentSpeed &lt; MaximumSpeedThreshold)
            {

            DesiredAngle = MaxSteerAngle - ((CurrentSpeed - MinimumSpeedThreshold) / SteerMultiplier);

            FL.MaxSteerAngle = DesiredAngle / 57.3f;
            FR.MaxSteerAngle = DesiredAngle / 57.3f;

            }
        if (CurrentSpeed &lt; MinimumSpeedThreshold)
        {
            FL.MaxSteerAngle = MaxSteerAngle / 57.3f;
            FR.MaxSteerAngle = MaxSteerAngle / 57.3f;
        }


    }




