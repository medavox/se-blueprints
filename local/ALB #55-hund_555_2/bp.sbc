// Gradient Wheel Steering and More by Beesoldier made for FSE

// setup: tag rear wheels with -GWS
// done

double speed1 = 50; // full rear wheel steering
double speed2 = 90; // no rear wheel steering

double maxsteeringangle = 10; // This is the rear wheel turn radius
double minsteeringangle = 0; // &lt; 0 inverts steering above speed2, its funny

bool manageinvertsteering = true; // false in case it messes up your setting
bool writetoscreen = true; // writes speed and max wheel angle to main screen
string wheeltag = "-GWS"; // add to name of suspensions for steering angle to be managed, preferably one pair, front or rear

string gyroname = "Flip Gyroscope"; // run script with 'invertgyro' to invert the gyros turn overrides

// configurable values above

List&lt;IMyShipController&gt; controllers = new List&lt;IMyShipController&gt;();
List&lt;IMyMotorSuspension&gt; allwheels = new List&lt;IMyMotorSuspension&gt;();
List&lt;IMyMotorSuspension&gt; managedwheels = new List&lt;IMyMotorSuspension&gt;();

IMyShipController driver;
IMyGyro gyro;

float maxangle;

public Program()
{
    Runtime.UpdateFrequency = UpdateFrequency.Once | UpdateFrequency.Update1;


    allwheels.Clear();
    GridTerminalSystem.GetBlocksOfType(allwheels);
    managedwheels.Clear();
    foreach (IMyMotorSuspension wheel in allwheels)
        if (wheel.CustomName.Contains(wheeltag))
        {
            managedwheels.Add(wheel);
        }


    controllers.Clear();
    GridTerminalSystem.GetBlocksOfType(controllers);
    foreach (IMyShipController controller in controllers)
    {
        {
            driver = controller;  // -Z forward +Y up +X right
        }
    }

    gyro = GridTerminalSystem.GetBlockWithName(gyroname) as IMyGyro;

    if (writetoscreen)
    {
        Me.GetSurface(0).ContentType = ContentType.TEXT_AND_IMAGE;
    }
}

public void Main(string argument, UpdateType updateSource)
{

    if (driver != null)
    {
        maxangle = (float)MathHelper.Clamp(-maxsteeringangle / 57.6 / (speed2 - speed1) * (driver.GetShipSpeed() - speed2), minsteeringangle / 57.6, maxsteeringangle / 57.6);

        Echo($"GWS 1.1 by Bee featuring\nsteering angle management\nanti-clang-slide\ninvert gyro by argument\n...for more info check editor");

        if (writetoscreen)
            Me.GetSurface(0).WriteText($"{(int)driver.GetShipSpeed()}m/s\n{(int)(maxangle * 57.6)}deg");
    }
    else
    {
        Echo("assign main cockpit and recompile");
        return;
    }

    foreach (IMyMotorSuspension wheel in managedwheels)
    {
        wheel.MaxSteerAngle = Math.Abs(maxangle);

        if (manageinvertsteering)
        {
            if (maxangle &lt; 0)
                wheel.InvertSteer = true;
            else
                wheel.InvertSteer = false;
        }
    }

    if (gyro != null)
        if (argument == "invertgyro")
        {
            gyro.Yaw = -gyro.Yaw;
            gyro.Pitch = -gyro.Pitch;
            gyro.Roll = -gyro.Roll;
        }


}</Program>
